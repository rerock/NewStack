<main class="splash-main">

  <section class="splash-content">

    <div class="temp">
      <% if signed_in? %>
        <h1>Stack</h1>
        Current User: <%= current_user.handle %><br>
        Current Team: <%= Team.all.find_by_id(params[:team_id]).name %><br>
        <%= link_to "Sign Out", root_url %>
      <% end %>
    </div>

    <div id="content"
      data-team-id="<%= params[:team_id] %>"
      data-user-id="<%= current_user.id %>"
      data-user-name="<%= current_user.handle %>"
      data-user-last-receivable-type="<%=current_user.last_receivable_type%>"
      data-user-last-receivable-id="<%=current_user.last_receivable_id%>"
    >
    </div>

  </section>


</main>
